{
    "regions": [
        "icn1"
    ],
    "functions": {
        "api/[...path].js": {
            "maxDuration": 60
        }
    },
    "rewrites": [
        {
            "source": "/api/edge/(.*)",
            "destination": "/api/edge/[...path]"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/[...path]"
        }
    ]
}